<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="Entity.CurrencyTradeRate" table="CurrencyTradeRate">  
    
    <id name="id">  
     <generator class="assigned"></generator>  
    </id>  
            
    <property name="currencyCodeExchanging"></property>  
    <property name="currencyRate"></property>  
	<property name="currencyCodeMain"></property>
	<property name="currencyRateDate"></property>
	<property name="cashToExchange"></property>
	<property name="cashExchanged"></property>
	
  </class>  
            
 </hibernate-mapping>  